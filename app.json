{
  "expo": {
    "name": "WETF-mobile",
    "slug": "WETF-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/WETFLogo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "UIBackgroundModes": ["audio"]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/WETFLogo.png",
        "backgroundColor": "#192378ff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/WETFLogo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/WETFLogo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#192378ff"
        }
      ],
      "expo-audio"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
